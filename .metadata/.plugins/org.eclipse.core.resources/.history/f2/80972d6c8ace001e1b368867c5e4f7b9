#ifndef INCLUDE_UART_USER_H_
#define INCLUDE_UART_USER_H_

#include "stm32f1xx_hal.h"

typedef enum
{
	START_ROBOT = 0,
	STOP_ROBOT,
	CHECK_THRESHOLD, //threshold
	START_MOTOR,
	POWER
} STATE_TX_PIN;

