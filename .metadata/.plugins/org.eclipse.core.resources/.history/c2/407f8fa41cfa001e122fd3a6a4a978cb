#include "stdio.h"

#include <handler_keyIN.h>

typedef enum
{
	UART_HANDLER_OKE,
	UART_HANDLER_ERROR
} STATE_TX_PIN;

void UART_Init_UART(UART_HandleTypeDef *huart);

STATE_TX_PIN UART_handler(uint8_t * rxBuffer, char* NUM, char* PIN);

